{
  "socketURL": "/",
  "socketMessageEvent": "RTCMultiConnection-Message",
  "socketCustomEvent": "RTCMultiConnection-Custom-Message",
  "port": "443",
  "enableLogs": "true",
  "autoRebootServerOnFailure": "true",
  "isUseHTTPs": "true",
  "sslKey": "/etc/ssl/private/nginx-selfsigned.key",
  "sslCert": "/etc/ssl/certs/nginx-selfsigned.crt",
  "ssl_cabundle": "/path/to/certificate.cabundle"
}
